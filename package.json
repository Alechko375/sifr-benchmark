{
  "name": "sifr-benchmark",
  "version": "1.0.0",
  "description": "Benchmark for evaluating LLM understanding of web UI across different formats",
  "type": "module",
  "main": "src/runner.js",
  "bin": {
    "sifr-bench": "./src/runner.js"
  },
  "scripts": {
    "bench": "node src/runner.js",
    "bench:quick": "node src/runner.js --models gpt-4o-mini --formats sifr,html_raw --runs 1",
    "bench:full": "node src/runner.js --runs 5",
    "analyze": "node src/analyze.js",
    "validate": "node src/validate.js"
  },
  "keywords": [
    "sifr",
    "llm",
    "benchmark",
    "web-ui",
    "ai-agents",
    "web-automation"
  ],
  "author": "SiFR Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "openai": "^4.70.0"
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/user/sifr-benchmark"
  }
}
